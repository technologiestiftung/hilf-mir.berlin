{
  "name": "wegweiser-frontend",
  "version": "1.2.0-rc.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run downloadCacheData && IS_BUILDING=true next build",
    "start": "next start",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest --maxWorkers=50% --coverage --collectCoverageFrom='./(src|pages)/**/*.ts(x)?' --collectCoverageFrom='!./(src|pages)/**/*.stories.ts(x)?' --collectCoverageFrom='!./src/mocks/**/*'",
    "test:watch": "npm run test -- --watch",
    "test:ci": "jest --runInBand",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "downloadCacheData": "npx ts-node src/scripts/downloadCacheData.ts"
  },
  "dependencies": {
    "date-fns": "2.29.3",
    "maplibre-gl": "2.1.6",
    "next": "12.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swiper": "8.4.3",
    "use-debounce": "8.0.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@technologiestiftung/semantic-release-config": "1.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "17.0.21",
    "@types/node-fetch": "2.6.2",
    "@types/react": "17.0.50",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "autoprefixer": "10.4.12",
    "babel-plugin-inline-react-svg": "2.0.1",
    "dotenv": "16.0.3",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.10",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-fetch-mock": "3.0.3",
    "node-fetch": "2.6.7",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "tailwindcss": "3.2.1",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.8.4"
  }
}
